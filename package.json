{
  "name": "server-api-data7",
  "author": "cesar carlos",
  "version": "1.0.11",
  "description": "Servidor data7 para integração",
  "bin": "./dist.loja/src/server.js",
  "main": "server.js",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "dev:nodemon": "cls && nodemon --watch 'src/' --exec 'ts-node --files --transpile-only src/server.ts' -e ts",
    "build:wind": "tsc && copyfiles ./.env ./dist/src/ && copyfiles ./src/certificates/**/* ./dist/ && copyfiles ./src/sql/**/* ./dist/",
    "build:wind:loja": "tsc --outDir ./dist.loja && copyfiles ./package.json ./dist.loja/src/ && copyfiles ./.env ./dist.loja/src/ && copyfiles ./src/certificates/**/* ./dist.loja/ && copyfiles ./src/sql/**/* ./dist.loja/",
    "format": "npx prettier './src/**/*.ts' './src/**/*.js' --write",
    "start": "node ./dist.loja/src/server.js",
    "test": "vitest"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "currency-formatter": "^1.5.9",
    "data7-pix": "^1.0.7",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "firebase-admin": "^13.0.2",
    "gn-api-sdk-typescript": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mssql": "^12.0.0",
    "node-localstorage": "^3.0.5",
    "promisify": "^0.0.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sybase": "^1.2.3",
    "tedious": "^18.6.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@firebase/firestore-types": "^3.0.3",
    "@types/cors": "^2.8.17",
    "@types/currency-formatter": "^1.5.4",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/mssql": "^9.1.6",
    "@types/node": "^24.3.1",
    "@types/node-localstorage": "^1.3.3",
    "@types/socket.io": "^3.0.2",
    "@types/tedious": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "jsdom": "^27.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^7.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  }
}
